{
  "name": "@tivac/eslint-config",
  "version": "2.4.0",
  "description": "ESLint config",
  "main": "./eslint.js",
  "keywords": [
    "eslint",
    "config",
    "tivac"
  ],
  "author": "Pat Cavit <npm@patcavit.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "prerelease": "npm run lint",
    "release": "npm version -m \"v%s\"",
    "postversion": "npm publish --access=public"
  },
  "devDependencies": {
    "eslint": "^4.19.1"
  },
  "eslintConfig": {
    "extends": [
      "./eslint.js"
    ],
    "env": {
      "node": true
    },
    "rules": {
      "quote-props": [
        "error",
        "consistent-as-needed"
      ]
    }
  }
}
